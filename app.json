{
  "expo": {
    "name": "nobreservice",
    "slug": "nobreservice",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "@react-native-firebase/app"
    ],
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "ts",
        "tsx",
        "svg"
      ]
    },
    "android": {
      "package": "com.nobreservice",
      "googleServicesFile": "./google-services.json"
    }
  }
}
